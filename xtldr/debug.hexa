	portNumber = 0
	baudRate = 0
	debugConfiguration = blGetConfigValue("DEBUG")
	if debugConfiguration and BlpStatus.DebugPort == 0 {
		debugPort = rtlTokenizeWideString(debugConfiguration, ";", lastPort)
		while debugPort != (0 as! PVOID) {
