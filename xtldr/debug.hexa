fun blpDebugPutChar(character USHORT) {
	var buffer USHORT[2]
	???? resolve ArraySubscriptExpr = character
	???? resolve ArraySubscriptExpr = 0
	hlComPortPutByte(BlpStatus.SerialPort, ???? resolve ArraySubscriptExpr)
	return (9223372036854775808 & 0)
}

